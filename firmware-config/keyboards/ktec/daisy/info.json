{
    "keyboard_name": "Daisy",
    "manufacturer": "KTEC",
    "url": "https://github.com/ktec-hq/Daisy",
    "maintainer": "qmk",

    "usb": {
        "vid": "0x4B50",
        "pid": "0xD7DC",
        "device_version": "5.0.1"
    },

    "features": {
        "secure": false,
        "bootmagic": true,
        "mousekey": false,
        "extrakey": true,
        "console": false,
        "command": false,
        "nkro": false,
        "backlight": true,
        "rgblight": true,
        "audio": false,

        "locking_support": true,
        "locking_resync": true
    },

    "build": {
        "lto": true
    },

    "processor": "atmega32u4",
    "bootloader": "atmel-dfu",

    "debounce": 5,

    "indicators": {
        // there is an additional indicator LED on the Esc key (wired to Pin D1)
        "caps_lock": "C6",
        "on_state": 0
    },

    "backlight": {
        "pin": "D0",
        "breathing": false,
        "levels": 6
    },

    "rgblight": {
        "led_count": 8,
        "pin": "C7",
        "hue_steps": 12,
        "saturation_steps": 25,
        "brightness_steps": 12,
        "max_brightness": 255
    },

    "diode_direction": "COL2ROW",
    "matrix_pins": {
        "rows": ["D2", "D3", "D5", "B7"],
        "cols": ["F0", "F1", "F4", "F5", "F6", "F7", "B6", "B5", "B4", "D7", "D6"]
    },

