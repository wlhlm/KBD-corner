{
    "manufacturer": "IBM",
    "keyboard_name": "Model M SSK with Yacobo PCB",
    "url": "https://github.com/sje-mse/yacobo",
    "maintainer": "wlhlm",

    "usb": {
        "vid": "0xFEED",
        "pid": "0xB156",
        "device_version": "1.0.0",

        "max_power": 100
    },

    "features": {
        "bootmagic": true,
        "mousekey": true,
        "extrakey": true,
        //"console": false,
        "console": true,
        //"command": false,
        "command": true,
        "nkro": false,
        "backlight": false,
        "audio": false
    },

    "bootmagic": {
        "matrix": [0, 2]
    },

    "build": {
        "lto": true
    },

    "development_board": "bluepill",

    "debounce": 5,

    "diode_direction": "COL2ROW",
    "matrix_pins": {
        "ghost": true,
        "rows": ["A1", "A2", "A3", "A4", "A5", "A6", "A7", "B0"],
        "cols": ["B9", "B8", "B7", "B6", "B5", "B4", "B3", "A15", "A10", "A9", "A8", "B15", "B14", "B13", "B12", "A0"]
    },

    "layout_aliases": {
        "LAYOUT_all": "LAYOUT_tkl_ansi_iso"
    },
    "layouts": {
        "LAYOUT_tkl_ansi_iso": {
            "layout": [
                {"label": "Esc", "x": 0, "y": 0, "matrix": [0, 2]},
                {"label": "F1", "x": 1.25, "y": 0, "matrix": [2, 3]},
                {"label": "F2", "x": 2.25, "y": 0, "matrix": [2, 4]},
